<idea-plugin>
    <id>io.cronenbergworld.symbol-idea-plugin</id>
    <name>Spec &amp; Math symbols</name>
    <vendor email="support@cronenbergworld.io" url="http://www.cronenbergworld.io">Cronenberg World</vendor>
    <description>_</description>
    <change-notes>_</change-notes>
    <version>_</version>
    <idea-version />

    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <defaultLiveTemplates file="/liveTemplates/Symbol.xml"/>
        <liveTemplateContext implementation="io.cronenbergworld.idea.plugin.symbol.SymbolTemplate"/>
        <defaultLiveTemplates file="/liveTemplates/SymbolMath.xml"/>
        <liveTemplateContext implementation="io.cronenbergworld.idea.plugin.symbol.SymbolMathTemplate"/>
        <defaultLiveTemplates file="/liveTemplates/SymbolEmoji.xml"/>
        <liveTemplateContext implementation="io.cronenbergworld.idea.plugin.symbol.SymbolEmojiTemplate"/>
        <defaultLiveTemplates file="/liveTemplates/LoremIpsum.xml"/>
        <liveTemplateContext implementation="io.cronenbergworld.idea.plugin.symbol.LoremIpsumTemplate"/>
        <liveTemplateMacro implementation="io.cronenbergworld.idea.plugin.symbol.FakeMacro"/>
        <defaultLiveTemplates file="/liveTemplates/Fake.xml"/>
        <liveTemplateContext implementation="io.cronenbergworld.idea.plugin.symbol.FakeTemplate"/>
    </extensions>

</idea-plugin>